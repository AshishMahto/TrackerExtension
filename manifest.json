{
  "name": "Tracker.gg - fix Teammates stats",
  "version": "1.0.0",
  "description": "On the 'Matches' tab, Tracker.gg will display stats of the Current Act only, instead of the default Current Episode.",
  "icons": {
    "16": "icons/trn16.png",
    "32": "icons/trn32.png",
    "48": "icons/trn48.png",
    "128": "icons/trn128.png"
  },
  "action": { "default_popup": "popup/popup.html" },
  "content_scripts": [
    {
      "matches": ["https://tracker.gg/valorant/profile/riot/*"],
      "js": ["script.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "manifest_version": 3
}
